{
  "name": "pyocd",
  "main": "./lib/pyocd",
  "version": "0.1.1",
  "description": "Flash your mbed / DAP-link device from Atom with pyOCD",
  "keywords": [
    "pyocd",
    "DAP-link",
    "mbed",
    "flash",
    "embedded"
  ],
  "activationCommands": {
    "atom-workspace": [
      "pyocd:toggle",
      "pyocd:flash",
      "pyocd:erase"
    ]
  },
  "repository": "https://github.com/wendlers/atom-pyocd",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-package-deps": "^4.0.1"
  },
  "package-deps": [
    "busy"
  ],
  "consumedServices": {
    "busy": {
      "versions": {
        "^1.0.0": "consumeBusy"
      }
    }
  }
}
